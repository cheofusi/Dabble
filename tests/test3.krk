soit lst2 = [10, 144, 17.67]
lst2[-1] = 2; lst2[1] = 294.1
afficher(lst2.size == 3, lst2[1] == lst2[-2], lst2[2] == 2)

soit lst3 = [lst2, [99, 999]]
